{
  "gitbook": "3.x.x",
  "root": "./docs",
  "plugins": [
    "yahei",
    "-lunr",
    "-search",
    "search-plus",
    "prism",
    "-highlight",
    "splitter",
    "-sharing",
    "sharing-plus",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "book-summary-scroll-position-saver",
